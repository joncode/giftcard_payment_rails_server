				<li class="itemDetail">
					<h5 class="wineTitle itemTitle">
						<%= w["item_name"] %>
					</h5>
					<h5 class="appDescription itemDescription">
						A blend of Petit Porqis and champagne, dipped in chocolate from central Paraguay
					</h5>
					<h5 class="winePrice itemPrice">
						<%= w["price"] %>
					</h5>
				</li>
				<% end %>
			</ul>
		</div>

		<div class="lineHoriz">
  	</div>

