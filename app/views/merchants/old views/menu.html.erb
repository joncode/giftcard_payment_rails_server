				<li class="editWrapper">
					<input type="text" placeholder="<%= w["item_name"] %>" class="wineTitle">
					<input type="text" placeholder="<%= w["description"] %>" class="wineDescription">
					<h5 class="dollarSignDetailList">$</h5>
					<input type="text" placeholder="<%= w["price"] %>" class="winePrice priceDetial gradient rounded">
					<div class="lineHoriz lineHorizInner"></div>
				</li>
				<% end %>
			</ul>

			<!-- Wine Menu View -->
			<ul class="wineListDetail wineMenuBox ulListDetail">
