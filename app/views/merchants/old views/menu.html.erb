<div class="beerWrapper menuCell">
	<div id="editPricesButtonBeer" class="buttonSmallerUp buttonSmallerDown">
		<div class="buttonHiderPrice"></div>
		<h5><a href="">edit price</a></h5>
	</div>
	<div id="editItemsButtonBeer" class="buttonSmallerDown buttonSmallerUp">
		<div class="buttonHiderItem"></div>
		<h5><a href="">edit items</a></h5>
	</div>
	<h5 class="menuSectionHeader">
		<%= @sections[1] %>
	</h5>
	<div class="buttonSave buttonSaveBeerPrice buttonSmallDown buttonSmallUp">
		<h5><a href="">save</a></h5>
	</div>

	<!-- Beer Edit Wrapper -->
	<div class="buttonSave buttonSaveBeerItem buttonSmallDown buttonSmallUp">
		<h5><a href="">save</a></h5>
	</div>

	<div class="beerEditWrapper rounded gradient">
		<ul class="beerList staticList">
			<% @beer.each do |b| %>
			<li class="staticButtonOff staticButton">

				<h5 class="staticButtonText">
					<%= b["item_name"] %> <br />

				</h5>
				<h5 class="staticButtonPrice">
					$<%= b["price"] %>
				</h5>
				<div class="plusIcon"></div>
				<div class="minusIcon"></div>
				<a href="" class="staticClickOff"></a>
			</li>
			<% end %>
		</ul>
	</div>

	<!-- Beer Menu Entry -->
	<ul class="beerItemsList ulSingleLineItem beerMenuBox">
		<% @beer.each do |b| %>
		<li class="singleLineItem">
			<h5 class="beerTitle itemTitle itemTitleSingle">
				<%= b["item_name"] %>
				 <span class="beerPrice priceSingleItem">
					- <%= b["price"] %>
				</span>
			</h5>
			<div class="priceEditLine">
				<div class="arrowDown"></div>
				<h5 class="dollarSignSmall">$</h5>
				<input type="text" placeholder="<%= b["price"] %>" class="beerPriceLine rounded gradient">
			</div>
		</li>
		<% end %>
  	</ul>
</div>  <!-- End Beer Wrapper -->


  	<div class="lineHoriz">
  	</div>

		<!-- Start Wine Wrapper -->
		<div class="wineWrapper menuCell">
			<div id="buttonEditWine" class="buttonSmallDown buttonSmallUp">
