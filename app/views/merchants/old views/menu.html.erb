<!-- Header -->
<% description =  "enter description here" %>
<div class="menuMakerWrapper"> <!-- Background Image -->
  <div class="logoCrooked"></div>
  <div class="headerImage"></div>
  <div class="lineVertHeader lineVert"></div>
  <div class="loggedInBox">
  	<h5>Logged in as: <a href="/home"><%= @current_user.fullname %></a></h5>
  </div>
  <div class="vendorTitle">
  	<h5 class="vendorTitleText"><a href="/merchants/<%= @provider.id %>"><%= @provider.name %></a></h5>
  	<h6 class="vendorAddress"><%= @provider.address %><br />
  			<%= @provider.city_state_zip %>
  	</h6>
  </div>
  <div class="lineHorizHeader lineHoriz"></div>
<!-- Beer Wrapper -->
  <!-- Column 1 Start -->
  	<div class="columnTopBlocker"></div>
  	  	<%= render 'merchant_options' %>
  <div class="columnOne">
  	<div class="sigDrinkWrapper menuCell">
    			<div class="sigDrinkMenu">
    				<div id="editButtonSigDrink" class="buttonSmallDown buttonSmallUp">
    					<h5><a href="">edit</a></h5>
    				</div>
    				<h5 class="menuSectionHeader">
    					<%= @sections[0] %>
    				</h5>
    					

    				<div class="sigDrinkEditWrapper editDescription rounded gradient">
    					<input type="text" placeholder="<%= @drink["item_name"] %>" class="sigDrinkTitle rounded" />
    					
    					<textarea placeholder="<%= @drink["description"] || description %>" class="sigDrinkDescription rounded" style="resize: none;"></textarea>
    					
    					<h5 class="menuSectionHeader dollarSign" >$</h5>
    					
    					<input type="text" placeholder="<%= @drink["price"] %>" rows="2" class="sigDrinkPrice rounded">
    					
    					<div id="saveButtonSigDrink" class="buttonSmallDown buttonSmallUp">
    						<h5><a href="">save</a></h5>
    					</div>
    				</div>

    				<!-- Signature Drink Menu Box -->
    				<div class="sigDrinkMenuBox">
    					<h5 class="sigDrinkTitle itemTitle">
    						<%= @drink["item_name"] %>
    					</h5>
    					<h5 class="sigDrinkDescription itemDescription">
    						<%= @drink["description"] || description %>
    					</h5>
    					<h5 class="sigDrinkPrice itemPrice">
    						<%= @drink["price"] %>
    					</h5>
    				</div>
    			</div>
    		</div>

		<div class="lineHoriz">
  	</div> 
